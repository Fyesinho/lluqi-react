    "react-odometerjs": "^2.0.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.17",
    "react-slick": "^0.16.0",
    "react-swipeable-views": "^0.12.12",
    "react-thunk": "^1.0.0",
